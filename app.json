{
  "expo": {
    "name": "Meetup",
    "slug": "Meetup",
    "version": "1.0.0",
    "scheme": "Meetup",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/default_avatar.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow MeetUp to use your location.",
          "locationWhenInUsePermission": "Show current location on map."
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1Ijoid29sdmVyMW4iLCJhIjoiY20zZnp6bWNzMDExczJoc21ocGh3NmVydCJ9.Ih4KVmfdb-yOBvCXaPmYmw"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/default_avatar.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/default_avatar.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wolver1n10.Meetup"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/default_avatar.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wolver1n10.Meetup"
    }
  }
}
